{
  "name": "backpack",
  "version": "1.0.3",
  "private": false,
  "author": {
    "name": "Greyson Mascarenhas",
    "email": "greysonmrx@gmail.com",
    "url": "https://github.com/greysonmrx"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "appId": "https://github.com/greysonmrx/Backpack",
    "productName": "Backpack",
    "copyright": "Copyright Â© 2019 Greyson",
    "mac": {
      "category": "public.education.utilities"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@unform/core": "^2.0.0",
    "@unform/web": "^2.0.0",
    "cross-env": "^7.0.2",
    "date-fns": "^2.11.0",
    "electron-is-dev": "^1.1.0",
    "history": "^4.10.1",
    "immer": "^6.0.2",
    "katex": "^0.11.1",
    "polished": "^3.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-icons": "^3.9.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-quill": "^1.3.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.0.1",
    "yup": "^0.28.3"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently  \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-build": "yarn build",
    "electron-build": "electron-builder -w",
    "commit": "git-cz"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.11",
    "@rescripts/rescript-env": "^0.0.10",
    "commitizen": "^4.0.3",
    "concurrently": "^4.1.2",
    "cz-conventional-changelog": "3.1.0",
    "electron": "^6.0.4",
    "electron-builder": "21.1.0",
    "typescript": "^3.5.3",
    "wait-on": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
